
<html lang="en">
  <head>
    <title>Home -EV Charge Hub</title>
    <meta charset="utf-8" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1, shrink-to-fit=no"
    />
    <link href="/css/style.css" rel="stylesheet" />
    <!-- Latest compiled and minified CSS -->
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN"
      crossorigin="anonymous"
    />
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL"
      crossorigin="anonymous"
    ></script>
    <link
      href="https://fonts.googleapis.com/css2?family=Poppins:wght@300&amp;display=swap"
      rel="stylesheet"
    />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css"
    />

    <!-- jQuery library -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>

    <!-- Popper JS -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
    <link rel="shortcut icon" type="image/x-icon" href="/Images/logo.png" />

    <style>
     @import url("https://fonts.googleapis.com/css2?family=Quicksand:wght@300;400;500;600;700&display=swap");
      
    </style>
    
  </head>

  <body>
    <!-- Full Desktop -->
    <header class="position-relative d-none d-sm-block m-2">
      <!-- <div class="container"> -->
      <div class="position-absolute ps-4" style="top: 20px">
        <a class="navbar-brand font-weight-bold" href="/home">
          <img
            src="Images/logo.png"
            class="img-fluid"
            style="max-width: 120px; max-height: 60px"
          />
        </a>
      </div>

      <nav
        class="navbar navbar-expand-sm navbar-light"
        style="margin-left: 150px"
      >
        <button
          class="navbar-toggler d-lg-none"
          type="button"
          data-toggle="collapse"
          data-target="#collapseNavbarid"
          aria-label="Toggle navigation"
        >
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="collapseNavbarid">
          <ul class="navbar-nav mt-2 d-flex justify-content-around">
            <li class="nav-item active">
              <a class="nav-link font-weight-bold" href="/home"
                >Home <span class="sr-only">(current)</span></a
              >
            </li>
            <li class="nav-item">
              <a class="nav-link font-weight-bold" href="/Joinus">Join Us</a>
            </li>

            <li class="nav-item dropdown">
              <a
                class="nav-link dropdown-toggle"
                href="#"
                role="button"
                data-bs-toggle="dropdown"
                aria-expanded="false"
              >
                Service
              </a>
              <ul class="dropdown-menu">
                <li><a class="dropdown-item" href="#">How to</a></li>
                <li><a class="dropdown-item" href="#">Booking</a></li>
                <li><a class="dropdown-item" href="#">Car Model</a></li>
                <li><a class="dropdown-item" href="/Map">Map</a></li>
                <li><a class="dropdown-item" href="/profile">Profile</a></li>
              </ul>
            </li>

            <li class="nav-item">
              <a class="nav-link font-weight-bold" href="#">Contact</a>
            </li>
          </ul>
        </div>
        <div>
          <ul class="navbar-nav ml-auto mt-2 d-flex justify-content-end">
            <% if (loggedIn) { %>
              <li class="nav-item">
                <a class="nav-link btn font-weight-bold text-danger" href="/LogOut">Log Out</a>
              </li>
              <% } %>

              <% if (!loggedIn) {%>
                <li class="nav-item">
                  <a class="nav-link btn font-weight-bold" href="/LogIn">Log In</a>
                </li>
                <li class="nav-item">
                  <a class="nav-link btn font-weight-bold" href="/SignUp">Sign Up</a>
                </li>
              <% } %>
          </ul>
        </div>
      </nav>

      <nav class="position-relative">
        <div class="d-flex justify-content-between">
          <div class="">
            <form
              class="form-inline my-lg-0 justify-content-start"
              style="margin-left: 150px"
            >
              <div class="input-group">
                <input
                  class="form-control"
                  type="text"
                  placeholder="Search"
                  style="
                    border: 1px solid #ccc;
                    border-radius: 20px;
                    width: 300px;
                  "
                />
                <div class="input-group-append">
                  <button type="submit" class="btn my-2 my-sm-0">
                    <i class="fas fa-search"></i>
                  </button>
                </div>
              </div>
            </form>
          </div>

          <div class="position-absolute" style="right: 2%">
            <div class="form-switch">
              <input
                type="checkbox"
                class="form-check-input selector-button"
                id="selector"
              />
              <label class="form-check-label" for="selector"></label>
            </div>
          </div>
        </div>
      </nav>

    </header>

    <!-- Small Desktop -->
    <header class="position-relative d-block d-sm-none">
      <div class="d-flex justify-content-between pt-3 pl-3 pr-3 p-4">
        <div class="">
          <a class="navbar-brand font-weight-bold" href="/home">
            <img
              src="/Images/logo.png"
              class="img-fluid"
              style="max-width: 120px; max-height: 60px"
            />
          </a>
        </div>

        <div>
          <button
            class="navbar-toggler d-lg-none"
            type="button"
            data-bs-toggle="collapse"
            data-bs-target="#collapseNavbarid"
            aria-label="Toggle navigation"
          >
            <div id="nav-icon3">
              <span></span>
              <span></span>
              <span></span>
              <span></span>
            </div>
          </button>
        </div>
      </div>

      <div>
        <div>
          <div class="collapse navbar-collapse" id="collapseNavbarid">
            <div class="p-3">
              <form
                class="form-inline justify-content-start"
                style="margin: 0px"
              >
                <div class="input-group flex-nowrap">
                  <input
                    class="form-control"
                    type="text"
                    placeholder="Search"
                    style="
                      border: 1px solid #ccc;
                      border-radius: 20px;
                      width: auto;
                      max-width: 300px;
                    "
                  />
                  <div class="input-group-append">
                    <button type="submit" class="btn my-2 my-sm-0">
                      <i class="fas fa-search"></i>
                    </button>
                  </div>
                </div>
              </form>
            </div>

             
            <ul class="navbar-nav d-flex justify-content-around p-3">
              <li class="nav-item active">
                <a class="nav-link font-weight-bold" href="/home"
                  >Home <span class="sr-only">(current)</span></a
                >
              </li>
              <li class="nav-item">
                <a class="nav-link font-weight-bold" href="/Joinus">Join Us</a>
              </li>

              <li class="nav-item dropdown">
                <a
                  class="nav-link dropdown-toggle"
                  href="#"
                  role="button"
                  data-bs-toggle="dropdown"
                  aria-expanded="false"
                >
                  Service
                </a>
                <ul class="dropdown-menu">
                  <li><a class="dropdown-item" href="#">How to</a></li>
                  <li><a class="dropdown-item" href="#">Booking</a></li>
                  <li><a class="dropdown-item" href="#">Car Model</a></li>
                  <li><a class="dropdown-item" href="/Map">Map</a></li>
                </ul>
              </li>

              <li class="nav-item">
                <a class="nav-link font-weight-bold" href="#">Contact</a>
              </li>

              <% if (loggedIn) { %>
                <li class="nav-item">
                  <a class="nav-link btn font-weight-bold text-danger" href="/LogOut">Log Out</a>
                </li>

                
                <% } %>

                <% if (!loggedIn) {%>
                  <li class="nav-item">
                    <a class="nav-link btn font-weight-bold" href="/LogIn">Log In</a>
                  </li>
                  <li class="nav-item">
                    <a class="nav-link btn font-weight-bold" href="/SignUp">Sign Up</a>
                  </li>
                <% } %>
                
            </ul>
           
            <div class="" style="padding: 1rem">
              <div class="form-switch">
                <input
                  type="checkbox"
                  class="form-check-input selector-button"
                  id="selectorb"
                />
                <label class="form-check-label" for="selectorb"></label>
              </div>
            </div>
          </div>
        </div>
      </div>
    </header>

    <!-- content -->
    <div class="container-fluid">
      <div class="row d-flex align-content-center" style="height: 500px">
        <div class="col-md-2"></div>
        <div class="col-md-8">
          <h1 class="display-2 text-center">Home Page</h1>
          <p class="text-center mt-5">Welcome, <%= UserData.email %> </p>
        </div>
      </div>
    </div>

    <div class="p-4 p-md-5 mb-4 rounded text-bg-dark">
        <div class="col-md-6 px-0">
          <h1 class="display-4 fst-italic">
            Home page
          </h1>
          <p class="lead mb-0">
            <a href="#" class="text-white fw-bold">Welcome, <%= UserData.name %> </a>
          </p>
        </div>
      </div>
    <!-- content -->



    <!-- footer -->

    <div class="container-fluid">
      <div class="d-flex flex-column justify-content-center">
        <h3 class="text-center">Want to partner with us?</h3>
        <p class="text-center"></p>
        <div class="row justify-content-center">
          <div class="col-12 col-sm-6 col-md-3">
            <button
              class="btn btn-block btn-outline-primary"
              style="border-radius: 20px; width: 100%; margin: 0 auto"
            >
              Get Started
            </button>
          </div>
        </div>
  
        <footer>
          <div class="container-fluid text-center mt-5">
    
            <ul class="list-unstyled social-media-icons text-center mt-2 list-unstyled">
              <li class="list-inline-item">
                <a href="#" class="social-media-icon">
                  <i class="fab fa-facebook-f"></i>
                </a>
              </li>
              <li class="list-inline-item">
                <a href="#" class="social-media-icon">
                  <i class="fab fa-twitter"></i>
                </a>
              </li>
              <li class="list-inline-item">
                <a href="#" class="social-media-icon">
                  <i class="fab fa-instagram"></i>
                </a>
              </li>
            </ul>
  
            <ul class="list-unstyled footer-menu">
              <li><a href="/home">Home</a></li>
              <li><a href="/Joinus">Join Us</a></li>
              <li><a href="#">Privacy</a></li>
              <li><a href="#">Terms Of Use</a></li>
              <li><a href="#">Help</a></li>
            </ul>
            
            <hr>
            <p class="copyright" style="color: #ffff; font-size: 12px;">
              &copy; 2023 <a href="/home">Ev Charge Hub.</a>  All rights reserved.
            </p>
            
          </div>
        </footer>
      </div>
    </div>

    <!-- footer -->

     <!-- dark mood -->
    <script>
      $(document).ready(function () {
        $(".selector-button").change(function () {
          $("body").toggleClass("bg-black");
          // ไปทำเป็นtransparent
          $("nav").toggleClass("navbar-dark ");
          $("header").toggleClass("navbar-dark");

          $(".custom-control-label").toggleClass("text-white");
          $("h1").toggleClass("text-white");
          $("h5").toggleClass("text-white");

          if ($(this).is(":checked")) {
            // เปลี่ยนสีไอคอนเป็นสีขาว
            $(".input-group-append .btn i").css("color", "#fff");
          } else {
            // เปลี่ยนสีไอคอนเป็นสีปกติ
            $(".input-group-append .btn i").css("color", "#000");
          }
        });
      });
    </script>

    <script>
      $(document).ready(function () {
        $("#nav-icon1,#nav-icon2,#nav-icon3,#nav-icon4").click(function () {
          $(this).toggleClass("open");
        });
      });
    </script>

    <!-- dark mood -->
  </body>
</html>
